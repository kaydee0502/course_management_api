
<%= render "home/nav_bar" %>
<p id="notice"><%= notice %></p>

<h1>Courses</h1>



<% @courses.each do |course| %>
<div class="card" style="width: 18rem;">
  <img class="card-img-top" src="data:image/jpeg;base64," alt="Card image cap">
  <div class="card-body">
    <h5 class="card-title"><%= course.name %></h5>
    <p class="card-text"><%= course.description %></p>
    <%= link_to 'Show', course,class:"btn btn-primary",style:"color : #FFF"%>
    <%= link_to 'Destroy', course, method: :delete, data: { confirm: 'Are you sure?' },class:"btn btn-error" %>
    <%= link_to 'Enroll', courses_enroll_path(:course => course.id), method: :post %>
  </div>
</div>
<% end %>

